<tr>  
  <td><span><%= get_index_page user_counter %> </span></td>
  <td><span><%= user.name %></span></td>
  <td><span><%= user.phone %></span></td>
  <td><span><%= user.idCart %></span></td>
  <td><span><%= user.email %></span></td>
  <td><span><%= user.count %></span></td>
  <td><span><%= user.room_name %></span></td>
  <td><span><%= user.status %></span></td>
  <td>
    <%= link_to user, title: t("show"), class: "btn btn-info btn-sm" do %>
      <i class="fa fa-eye"></i>
    <% end %>
    <%= link_to edit_user_path(user), title: t("edit"), class: "btn btn-warning btn-sm" do %>
      <i class="fa fa-edit"></i>
    <% end %>
    <%= link_to user, title: t("delete"), method: :delete, data: {confirm: t("sure")}, class: "btn btn-danger btn-sm" do %>
      <i class="fa fa-trash"></i>
    <% end %>
  </td>
</tr>
